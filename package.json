{
  "name": "ng-daterangepicker",
  "version": "0.2.1",
  "description": "Angular PayPal Datepicker",
  "main": "bundles/ng-daterangepicker.umd.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "start": "webpack-dev-server --env.client --env.serve --progress",
    "start:aot": "webpack-dev-server --env.client --env.serve --env.aot --progress",
    "build": "tsc && webpack --env.client --env.aot --progress --hide-modules",
    "build:prod": "webpack --env.prod --env.aot -p --progress --hide-modules",
    "prebuild": "npm run clean",
    "prebuild:prod": "npm run clean",
    "clean": "rimraf dist lib bundles ./**/*.ngsummary.*",
    "prepublishOnly": "tsc -p tsconfig.bundle.json && webpack --config webpack.bundle.js -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jkuri/ng-daterangepicker.git"
  },
  "keywords": [
    "Angular",
    "date",
    "datepicker",
    "range",
    "daterange",
    "daterangepicker"
  ],
  "author": "Jan Kuri <jkuri88@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jkuri/ng-daterangepicker/issues"
  },
  "homepage": "https://github.com/jkuri/ng-daterangepicker#readme",
